{{#resource "m:core" "base" type="AWS::RDS::DBCluster"}}
  {{#helper "m:core" "properties"}}
    {{helper "m:core" "property" "Engine"}}
    {{helper "m:core" "property" "AvailabilityZones" optional=true}}
    {{helper "m:core" "property" "BackupRetentionPeriod" type="number" optional=true}}
    {{helper "m:core" "property" "DatabaseName" optional=true}}
    {{helper "m:core" "property" "DBClusterParameterGroupName" valueVariable="dbClusterParameterGroupName" optional=true}}
    {{helper "m:core" "property" "DBSubnetGroupName" valueVariable="dbSubnetGroupName" optional=true}}
    {{helper "m:core" "property" "EngineVersion" optional=true}}
    {{helper "m:core" "property" "KmsKeyId" optional=true}}
    {{helper "m:core" "property" "MasterUsername" optional=true}}
    {{helper "m:core" "property" "MasterUserPassword" optional=true}}
    {{helper "m:core" "property" "Port" optional=true type="number"}}
    {{helper "m:core" "property" "PreferredBackupWindow" optional=true}}
    {{helper "m:core" "property" "PreferredMaintenanceWindow" optional=true}}
    {{helper "m:core" "property" "SnapshotIdentifier" optional=true}}
    {{helper "m:core" "property" "StorageEncrypted" optional=true}}
    {{helper "m:core" "property" "Tags" optional=true}}
    {{helper "m:core" "property" "VpcSecurityGroupIds" optional=true}}
  {{/helper}}
{{/resource}}
